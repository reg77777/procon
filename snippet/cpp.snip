	struct UF{
	  vector<int>p; //p[i]=$B%N!<%I(Bi$B$N?F(B
	  vector<int>r; //rank[i]=$B%N!<%I(Bi$B$N%i%s%/(B
	  UF(int sz){
	    r.resize(sz,0);
	    p.resize(sz);
	    for(int i=0;i<sz;i++)p[i]=i;
	  }
	  void u(int x,int y){ //1
	    x=root(x);
	    y=root(y);
	    if(r[x]==r[y])r[x]++;
	    if(r[x]>r[y])p[y]=x;
	    else p[x]=y;
	  }
	  int root(int x){
	    if(p[x]==x)return x;
	    return root(x=p[x]);
	  }
	  bool isu(int x,int y){ //2
	    return root(x)==root(y);
	  }
	};
